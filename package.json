{
  "name": "blackpepperswing",
  "version": "0.10.0",
  "description": "Black Pepper Swing website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gulp": "node_modules/gulp/bin/gulp.js",
    "clean": "rm -rf node_modules package-lock.json vendor .bundle",
    "theme:build": "gem build jekyll-theme-black-pepper-swing.gemspec",
    "build": "npm run gulp images && JEKYLL_ENV=production bundle exec jekyll build --config _config.yml",
    "build-staging": "npm run gulp images && JEKYLL_ENV=dev bundle exec jekyll build --config _config.yml,_config_staging.yml",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karrirasinmaki/blackpepperswing-www.git"
  },
  "keywords": [
    "jekyll",
    "feelingresponsive",
    "foundation"
  ],
  "author": "Karri Rasinm√§ki (Intraktio Ltd)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karrirasinmaki/blackpepperswing-www/issues"
  },
  "homepage": "https://github.com/karrirasinmaki/blackpepperswing-www#readme",
  "devDependencies": {
    "firebase-tools": "^7.1.0",
    "gulp": "^4.0.0",
    "gulp-changed": "^4.0.3",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-util": "^3.0.8",
    "json-to-pretty-yaml": "^1.2.2",
    "run-sequence": "^2.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "mozjpeg": "^7.1.0"
  }
}
